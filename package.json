{
  "name": "coworking-management-portal",
  "version": "1.0.0",
  "description": "A co-working management portal with Next.js, Express, and PostgreSQL",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "next dev",
    "dev:server": "nodemon server/index.js",
    "dev:full": "concurrently \"npm run dev:client\" \"npm run dev:server\" \"npm run cron:start\"",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "node scripts/setup-db.js",
    "cron:start": "node scripts/cron-job.js",
    "cron:test": "node -e \"require('./scripts/cron-job').runManualCheck()\"",
    "task-cron:start": "node scripts/task-cron.js",
    "task-cron:test": "node -e \"require('./scripts/task-cron').runManualTaskCheck()\"",
    "server:start": "node scripts/start-server.js"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.7.1",
    "@supabase/supabase-js": "^2.75.0",
    "@types/nodemailer": "^7.0.2",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.0.4",
    "next-auth": "^4.24.5",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.10.1",
    "postcss": "^8.4.32",
    "postgres": "^3.4.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-query": "^3.39.3",
    "tailwindcss": "^3.3.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.1",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "nextjs",
    "express",
    "postgresql",
    "prisma",
    "coworking",
    "management"
  ],
  "author": "Your Name",
  "license": "MIT"
}
